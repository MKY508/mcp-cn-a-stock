{
  "name": "MCP CN A-Stock Server",
  "image": "mcr.microsoft.com/devcontainers/python:3.12",
  
  "postCreateCommand": "pip install -r requirements.txt && python main.py --transport=sse --port 8000",
  
  "forwardPorts": [8000],
  "portsAttributes": {
    "8000": {
      "label": "MCP Server",
      "onAutoForward": "notify",
      "visibility": "public"
    }
  },
  
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  }
}